{
  "name": "terrario",
  "version": "0.8.1",
  "description": "A simple parser combinator library with TypeScript.",
  "type": "module",
  "main": "./built/index.js",
  "types": "./built/index.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "npx tsc --outdir built/cjs --module CommonJS && npm run build:cjs:rename",
    "build:cjs:rename": "node -e \"require('fs').renameSync('./built/cjs/index.js', './built/cjs/index.cjs')\"",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marihachi/terrario.git"
  },
  "author": "Marihachi",
  "contributors": [
    "ThinaticSystem <git@thinaticsystem.com> (https://thinaticsystem.com/)"
  ],
  "exports": {
    "import": "./built/index.js",
    "require": "./built/cjs/index.cjs",
    "default": "./built/index.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "29.0.3",
    "@types/node": "18.7.18",
    "jest": "29.0.3",
    "ts-jest": "29.0.1",
    "typescript": "4.8.3"
  },
  "files": [
    "built/**/*.js",
    "built/**/*.d.ts",
    "CHANGELOG.md"
  ]
}
